<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             x:Class="ChartControlsThemeDemo.SfSunburstChartThemeKeys">

    <x:String x:Key="SfSunburstChartTheme">CommonTheme</x:String>

    <Color x:Key="SfSunburstChartBackground"/>

    <!--Tooltip-->
    <Color x:Key="SfSunburstChartTooltipBackground"/>
    <Color x:Key="SfSunburstChartTooltipTextColor"/>
    <sys:Double x:Key="SfSunburstChartTooltipTextFontSize"/>

    <!--Legend-->
    <Color x:Key="SfSunburstChartLegendTextColor"/>
    <sys:Double x:Key="SfSunburstChartLegendFontSize"/>

</ResourceDictionary>
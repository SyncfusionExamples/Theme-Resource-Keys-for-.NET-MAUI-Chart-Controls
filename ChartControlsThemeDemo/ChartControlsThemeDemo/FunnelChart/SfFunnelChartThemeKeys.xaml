<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             x:Class="ChartControlsThemeDemo.SfFunnelChartThemeKeys">

    <x:String x:Key="SfFunnelChartTheme">CommonTheme</x:String>

    <Color x:Key="SfFunnelChartBackground"/>

            <!--Tooltip-->
    <Color x:Key="SfFunnelChartTooltipBackground"/>
    <Color x:Key="SfFunnelChartTooltipTextColor"/>
    <sys:Double x:Key="SfFunnelChartTooltipTextFontSize"/>

            <!--Legend-->
    <Color x:Key="SfFunnelChartLegendTextColor"/>
    <sys:Double x:Key="SfFunnelChartLegendFontSize"/>
    <Color x:Key="SfFunnelChartLegendDisableBrush"/>

</ResourceDictionary>
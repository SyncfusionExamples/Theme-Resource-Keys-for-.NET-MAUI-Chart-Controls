<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             x:Class="ChartControlsThemeDemo.SfPyramidChartThemeKeys">

    <x:String x:Key="SfPyramidChartTheme">CommonTheme</x:String>

    <Color x:Key="SfPyramidChartBackground"/>
    
            <!--Tooltip-->
    <Color x:Key="SfPyramidChartTooltipBackground" />
    <Color x:Key="SfPyramidChartTooltipTextColor"/>
    <sys:Double x:Key="SfPyramidChartTooltipTextFontSize"/>

            <!--Legend-->
    <Color x:Key="SfPyramidChartLegendTextColor"/>
    <sys:Double x:Key="SfPyramidChartLegendFontSize"/>
    <Color x:Key="SfPyramidChartLegendDisableBrush"/>

</ResourceDictionary>
<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             x:Class="ChartControlsThemeDemo.SfCircularChartThemeKeys">

    <x:String x:Key="SfCircularChartTheme">CommonTheme</x:String>
    
    <Color x:Key="SfCircularChartBackground"/>

             <!--Tooltip-->
    <Color x:Key="SfCircularChartTooltipBackground"/>
    <Color x:Key="SfCircularChartTooltipTextColor"/>
    <sys:Double x:Key="SfCircularChartTooltipTextFontSize"/>

             <!--Legend-->
    <Color x:Key="SfCircularChartLegendTextColor"/>
    <sys:Double x:Key="SfCircularChartLegendFontSize"/>
    <Color x:Key="SfCircularChartLegendDisableBrush"/>

</ResourceDictionary>